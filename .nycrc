{
  "all": true,
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "**/*.test.js",
    "**/*.spec.js",
    "**/node_modules/**",
    "**/test/**",
    "**/tests/**",
    "**/__tests__/**",
    "**/coverage/**",
    "**/web/**",
    "src/cli/**",
    "src/index.js",
    "src/app.js"
  ],
  "reporter": [
    "text",
    "lcov",
    "html",
    "json-summary"
  ],
  "report-dir": "coverage",
  "temp-dir": ".nyc_output",
  "check-coverage": false,
  "branches": 25,
  "lines": 25,
  "functions": 25,
  "statements": 25,
  "watermarks": {
    "lines": [50, 80],
    "functions": [50, 80],
    "branches": [50, 80],
    "statements": [50, 80]
  }
}